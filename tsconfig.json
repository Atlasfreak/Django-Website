{
    "compilerOptions": {
        "target": "esnext",
        "lib": [
            "DOM",
            "ESNext"
        ],
        "allowSyntheticDefaultImports": true,
        "downlevelIteration": true,
        "forceConsistentCasingInFileNames": true,
        "moduleResolution": "node",
        "importsNotUsedAsValues": "remove",
        "sourceMap": true
    }
}